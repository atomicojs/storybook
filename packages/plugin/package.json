{
    "name": "@atomico/storybook",
    "version": "2.1.0",
    "public": true,
    "description": "Render Atomico's virtualDOM in Storybook",
    "type": "module",
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "step:types": "tsc",
        "step:build": "library src/**",
        "step:exports": "exports types/** lib/** --main core",
        "build": "npm run step:types && npm run step:build && npm run step:exports",
        "prepublishOnly": "npm run exports"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@atomico/react": "*",
        "@atomico/tsconfig": "*",
        "@storybook/addons": "*",
        "@storybook/docs-tools": "*",
        "atomico": "^1.70.0"
    },
    "peerDependencies": {
        "@atomico/react": "*",
        "@storybook/addons": "*",
        "@storybook/docs-tools": "*",
        "atomico": "*"
    },
    "peerDependenciesMeta": {
        "@atomico/react": {
            "optional": true
        }
    }
}
