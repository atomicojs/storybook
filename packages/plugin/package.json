{
    "name": "@atomico/storybook",
    "version": "2.2.4",
    "public": true,
    "description": "Render Atomico's virtualDOM in Storybook",
    "type": "module",
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "build:types": "tsc",
        "build:compile": "library src/**",
        "build:exports": "exports types/** lib/** --main core",
        "dev:types": "tsc --watch",
        "dev:exports": "exports types/** lib/** --main core --watch",
        "prepublishOnly": "npm run step:types && npm run step:build && npm run step:exports"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@atomico/react": "*",
        "@atomico/tsconfig": "*",
        "@storybook/addons": "*",
        "@storybook/docs-tools": "*",
        "atomico": "^1.70.0"
    },
    "peerDependencies": {
        "@atomico/react": "*",
        "@storybook/addons": "*",
        "@storybook/docs-tools": "*",
        "atomico": "*"
    },
    "peerDependenciesMeta": {
        "@atomico/react": {
            "optional": true
        }
    }
}
