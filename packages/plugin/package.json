{
    "name": "@atomico/storybook",
    "version": "2.5.0",
    "public": true,
    "description": "Render Atomico's virtualDOM in Storybook",
    "type": "module",
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "build:types": "tsc",
        "build:compile": "library src/**",
        "build:exports": "exports types/** lib/** --main core",
        "dev:types": "tsc --watch",
        "dev:exports": "exports types/** src/** --main core --watch"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@atomico/react": "*",
        "@atomico/tsconfig": "*",
        "@storybook/addons": "*",
        "@storybook/docs-tools": "*",
        "atomico": "^1.72.0"
    },
    "peerDependencies": {
        "@atomico/react": "*",
        "@storybook/addons": "*",
        "@storybook/docs-tools": "*",
        "atomico": "*"
    },
    "peerDependenciesMeta": {
        "@atomico/react": {
            "optional": true
        }
    },
    "main": "src/core.ts",
    "module": "src/core.ts",
    "types": "types/core.d.ts",
    "exports": {
        "./core": "./src/core.ts",
        "./decorator": "./src/decorator.tsx",
        "./options": "./src/options.ts",
        "./react": "./src/react.ts",
        "./serialize-dom": "./src/serialize-dom.ts",
        "./serialize-jsx": "./src/serialize-jsx.ts",
        "./types": "./src/types.ts",
        ".": "./src/core.ts"
    },
    "typesVersions": {
        "*": {
            "core": [
                "types/core.d.ts"
            ],
            "decorator": [
                "types/decorator.d.ts"
            ],
            "options": [
                "types/options.d.ts"
            ],
            "react": [
                "types/react.d.ts"
            ],
            "serialize-dom": [
                "types/serialize-dom.d.ts"
            ],
            "serialize-jsx": [
                "types/serialize-jsx.d.ts"
            ],
            "types": [
                "types/types.d.ts"
            ]
        }
    }
}
