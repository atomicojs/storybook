{
    "name": "@atomico/storybook",
    "version": "2.4.0",
    "public": true,
    "description": "Render Atomico's virtualDOM in Storybook",
    "type": "module",
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "build:types": "tsc",
        "build:compile": "library src/**",
        "build:exports": "exports types/** lib/** --main core",
        "dev:types": "tsc --watch",
        "dev:exports": "exports types/** src/** --main core --watch"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@atomico/react": "*",
        "@atomico/tsconfig": "*",
        "@storybook/addons": "*",
        "@storybook/docs-tools": "*",
        "atomico": "^1.70.0"
    },
    "peerDependencies": {
        "@atomico/react": "*",
        "@storybook/addons": "*",
        "@storybook/docs-tools": "*",
        "atomico": "*"
    },
    "peerDependenciesMeta": {
        "@atomico/react": {
            "optional": true
        }
    },
    "main": "lib/core.js",
    "module": "lib/core.js",
    "types": "types/core.d.ts",
    "exports": {
        "./core": "./lib/core.js",
        "./decorator": "./lib/decorator.js",
        "./options": "./lib/options.js",
        "./react": "./lib/react.js",
        "./serialize-dom": "./lib/serialize-dom.js",
        "./serialize-jsx": "./lib/serialize-jsx.js",
        "./types": "./lib/types.js",
        ".": "./lib/core.js"
    },
    "typesVersions": {
        "*": {
            "core": [
                "types/core.d.ts"
            ],
            "decorator": [
                "types/decorator.d.ts"
            ],
            "options": [
                "types/options.d.ts"
            ],
            "react": [
                "types/react.d.ts"
            ],
            "serialize-dom": [
                "types/serialize-dom.d.ts"
            ],
            "serialize-jsx": [
                "types/serialize-jsx.d.ts"
            ],
            "types": [
                "types/types.d.ts"
            ]
        }
    }
}
