{
    "name": "@atomico/storybook",
    "version": "1.9.5",
    "public": true,
    "description": "Render Atomico's virtualDOM in Storybook",
    "type": "module",
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "exports": "exports src/*.ts --types --exports --main core --watch",
        "prepublishOnly": "npm run exports"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@atomico/react": "*",
        "@atomico/tsconfig": "*",
        "@storybook/addons": "*",
        "atomico": "^1.70.0"
    },
    "peerDependencies": {
        "@atomico/react": "*",
        "@storybook/addons": "*",
        "atomico": "*"
    },
    "peerDependenciesMeta": {
        "@atomico/react": {
            "optional": true
        }
    },
    "module": "./dist/core.js",
    "exports": {
        "./core": "./dist/core.js",
        ".": "./dist/core.js",
        "./decorator": "./dist/decorator.js",
        "./serialize": "./dist/serialize.js",
        "./types": "./dist/types.js",
        "./options": "./dist/options.js",
        "./react": "./dist/react.js",
        "./serialize-dom": "./dist/serialize-dom.js",
        "./serialize-jsx": "./dist/serialize-jsx.js"
    },
    "types": "types/core.d.ts",
    "typesVersions": {
        "*": {
            "core": [
                "./types/core.d.ts"
            ],
            "decorator": [
                "./types/decorator.d.ts"
            ],
            "serialize": [
                "./types/serialize.d.ts"
            ],
            "types": [
                "./types/types.d.ts"
            ],
            "options": [
                "./types/options.d.ts"
            ],
            "react": [
                "./types/react.d.ts"
            ],
            "serialize-dom": [
                "./types/serialize-dom.d.ts"
            ],
            "serialize-jsx": [
                "./types/serialize-jsx.d.ts"
            ]
        }
    }
}
