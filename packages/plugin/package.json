{
    "name": "@atomico/storybook",
    "version": "2.5.1",
    "public": true,
    "description": "Render Atomico's virtualDOM in Storybook",
    "type": "module",
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "build:compile": "tsc",
        "build:exports": "exports dist/** --main core",
        "dev:compile": "tsc --watch",
        "dev:exports": "exports dist/** --main core --watch"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@atomico/react": "*",
        "@atomico/tsconfig": "*",
        "@storybook/addons": "*",
        "@storybook/docs-tools": "*",
        "atomico": "^1.72.0"
    },
    "peerDependencies": {
        "@atomico/react": "*",
        "@storybook/addons": "*",
        "@storybook/docs-tools": "*",
        "atomico": "*"
    },
    "peerDependenciesMeta": {
        "@atomico/react": {
            "optional": true
        }
    },
    "main": "dist/core.js",
    "module": "dist/core.js",
    "types": "dist/core.d.ts",
    "exports": {
        "./core": "./dist/core.js",
        "./decorator": "./dist/decorator.js",
        "./options": "./dist/options.js",
        "./react": "./dist/react.js",
        "./serialize-dom": "./dist/serialize-dom.js",
        "./serialize-jsx": "./dist/serialize-jsx.js",
        "./types": "./dist/types.js",
        ".": "./dist/core.js"
    },
    "typesVersions": {
        "*": {
            "core": [
                "dist/core.d.ts"
            ],
            "decorator": [
                "dist/decorator.d.ts"
            ],
            "options": [
                "dist/options.d.ts"
            ],
            "react": [
                "dist/react.d.ts"
            ],
            "serialize-dom": [
                "dist/serialize-dom.d.ts"
            ],
            "serialize-jsx": [
                "dist/serialize-jsx.d.ts"
            ],
            "types": [
                "dist/types.d.ts"
            ]
        }
    }
}