{
    "name": "@atomico/storybook",
    "version": "1.5.4",
    "public": true,
    "description": "Render Atomico's virtualDOM in Storybook",
    "type": "module",
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "exports": "exports src/*.ts --types --exports --main core",
        "prepublishOnly": "npm run exports"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "atomico": "latest",
        "@storybook/addons": "latest",
        "@atomico/tsconfig": "latest"
    },
    "peerDependencies": {
        "atomico": "*",
        "@storybook/addons": "*"
    },
    "module": "./dist/core.js",
    "exports": {
        "./core": "./dist/core.js",
        ".": "./dist/core.js",
        "./decorator": "./dist/decorator.js",
        "./serialize": "./dist/serialize.js",
        "./types": "./dist/types.js"
    },
    "types": "types/core.d.ts",
    "typesVersions": {
        "*": {
            "core": [
                "./types/core.d.ts"
            ],
            "decorator": [
                "./types/decorator.d.ts"
            ],
            "serialize": [
                "./types/serialize.d.ts"
            ],
            "types": [
                "./types/types.d.ts"
            ]
        }
    }
}
