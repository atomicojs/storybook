{
    "name": "@atomico/storybook",
    "version": "1.3.0",
    "public": true,
    "description": "Render Atomico's virtualDOM in Storybook",
    "type": "module",
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "exports": "exports src/*.ts --types --exports --main decorator",
        "prepublishOnly": "npm run exports"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "atomico": "latest",
        "@storybook/addons": "latest",
        "@atomico/tsconfig": "latest"
    },
    "peerDependencies": {
        "atomico": "*",
        "@storybook/addons": "*"
    },
    "module": "./dist/decorator.js",
    "exports": {
        "./decorator": "./dist/decorator.js",
        ".": "./dist/decorator.js",
        "./serialize": "./dist/serialize.js",
        "./utils": "./dist/utils.js"
    },
    "types": "types/decorator.d.ts",
    "typesVersions": {
        "*": {
            "decorator": [
                "./types/decorator.d.ts"
            ],
            "serialize": [
                "./types/serialize.d.ts"
            ],
            "utils": [
                "./types/utils.d.ts"
            ]
        }
    }
}
